{
  "expo": {
    "name": "clothes_app",
    "slug": "clothes_app",
    "scheme": "clothes_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo_full.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.fenol64.clothes_app"
    },
    "web": {
      "favicon": "./assets/logo_icon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "O app precisa accessar suas fotos.",
          "cameraPermission": "O app precisa accessar sua c√¢mera."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c4503362-c236-4930-bcab-35790eedf713"
      }
    }
  }
}
